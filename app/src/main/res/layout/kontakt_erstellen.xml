    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/bild_3"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="650dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/bild_4"
        tools:layout_editor_absoluteX="251dp"
        tools:layout_editor_absoluteY="650dp" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/bild_8"
        tools:layout_editor_absoluteX="331dp"
        tools:layout_editor_absoluteY="650dp" />

    <EditText
        android:id="@+id/editTextFirstName"
        android:layout_width="311dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="First Name"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="230dp" />

    <EditText
        android:id="@+id/editTextLastName"
        android:layout_width="311dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Last Name"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="280dp" />

    <EditText
        android:id="@+id/editTextCompany"
        android:layout_width="311dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Company"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="330dp" />

    <EditText
        android:id="@+id/editTextRegManager"
        android:layout_width="311dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Regional Manager"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="380dp" />

    <EditText
        android:id="@+id/editTextNumberMobilePhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="+49 0000 00000000"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="430dp" />

    <EditText
        android:id="@+id/editTextMobile"
        android:layout_width="311dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Mobile"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="480dp" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="311dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Phone"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="530dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="63dp"
        android:layout_height="57dp"
