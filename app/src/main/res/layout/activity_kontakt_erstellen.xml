<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.kontakt_erstellen">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="431dp"
        android:layout_height="79dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/textView_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/home"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.47"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="101dp"
            android:layout_height="89dp"

            android:contentDescription="@string/logo"
            app:layout_constraintBottom_toBottomOf="@+id/constraintLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.83"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/constraintLayout"
            app:layout_constraintVertical_bias="0.79"
            app:srcCompat="@drawable/bild_11" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="63dp"
        android:layout_height="57dp"
        android:src="@drawable/bild_9"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Businesscard scannen"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout4"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:src="@drawable/bild_20"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.658"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="59dp"
        android:layout_height="45dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/bild_21"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.312"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="425dp"
        android:layout_height="88dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/kontakteButton"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:contentDescription="@string/img_kontakte"

            app:srcCompat="@drawable/komponente_4__9" />

        <ImageButton
            android:id="@+id/homeButton"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:contentDescription="@string/img_home"

            app:srcCompat="@drawable/komponente_310" />

        <ImageButton

            android:id="@+id/mapsButton"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:contentDescription="@string/img_map"

            app:srcCompat="@drawable/komponente_59" />

        <ImageButton
            android:id="@+id/searchButton"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:contentDescription="@string/img_search"

            app:srcCompat="@drawable/komponente_62" />

        <ImageButton
            android:id="@+id/menuButton"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:contentDescription="@string/img_menu"

            app:srcCompat="@drawable/komponente_2_9" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="376dp"
        android:layout_height="292dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

        <EditText
            android:id="@+id/editTextFirstName"
            android:layout_width="311dp"
            android:layout_height="38dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="First Name"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="230dp"
            android:importantForAutofill="no" />

        <EditText
            android:id="@+id/editTextLastName"
            android:layout_width="311dp"
            android:layout_height="38dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Last Name"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="280dp"
            android:importantForAutofill="no" />

        <EditText
            android:id="@+id/editTextCompany"
            android:layout_width="311dp"
            android:layout_height="38dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Company"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="330dp"
            android:importantForAutofill="no" />

        <EditText
            android:id="@+id/editTextRegManager"
            android:layout_width="311dp"
            android:layout_height="38dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Regional Manager"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="380dp"
            android:importantForAutofill="no" />

        <EditText
            android:id="@+id/editTextNumberMobilePhone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="numberDecimal"
            android:text="+49 0000 00000000"
            tools:layout_editor_absoluteX="40dp"
            tools:layout_editor_absoluteY="430dp"
            android:importantForAutofill="no" />

        <EditText
            android:id="@+id/editTextMobile"
            android:layout_width="311dp"
            android:layout_height="38dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Mobile"
            tools:layout_editor_absoluteX="40dp"
            tools:layout_editor_absoluteY="480dp"
            android:importantForAutofill="no" />

        <EditText
            android:id="@+id/editTextPhone"
            android:layout_width="311dp"
            android:layout_height="38dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Phone"
            tools:layout_editor_absoluteX="40dp"
            tools:layout_editor_absoluteY="530dp"
            android:importantForAutofill="no" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>