<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.kontakt_erstellen">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="425dp"
        android:layout_height="88dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/kontakteButton"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:contentDescription="@string/img_kontakte"

            app:srcCompat="@drawable/komponente_4__9" />

        <ImageButton
            android:id="@+id/homeButton"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:contentDescription="@string/img_home"

            app:srcCompat="@drawable/komponente_310" />

        <ImageButton

            android:id="@+id/mapsButton"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:contentDescription="@string/img_map"

            app:srcCompat="@drawable/komponente_59" />

        <ImageButton
            android:id="@+id/searchButton"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:contentDescription="@string/img_search"

            app:srcCompat="@drawable/komponente_62" />

        <ImageButton
            android:id="@+id/menuButton"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:contentDescription="@string/img_menu"

            app:srcCompat="@drawable/komponente_2_9" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="432dp"
        android:layout_height="770dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="431dp"
            android:layout_height="79dp"
            android:background="#FFFFFF">

            <TextView
                android:id="@+id/textView_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kontakte"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.47"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="101dp"
                android:layout_height="89dp"

                android:contentDescription="@string/logo"
                app:layout_constraintBottom_toBottomOf="@+id/constraintLayout"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.83"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/constraintLayout"
                app:layout_constraintVertical_bias="0.79"
                app:srcCompat="@drawable/bild_11" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <LinearLayout
            android:id="@+id/linearLayout6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/abortButton"
                android:layout_width="53dp"
                android:layout_height="47dp"
                android:src="@drawable/bild_21" />

            <Button
                android:id="@+id/scanCardButton"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="Businesscard scannen" />

            <ImageButton
                android:id="@+id/acceptButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/bild_20" />

        </LinearLayout>

        <ScrollView
            android:id="@+id/scrollView3"
            android:layout_width="400dp"
            android:layout_height="611dp">

            <LinearLayout
                android:id="@+id/linearLayout4"
                android:layout_width="match_parent"
                android:layout_height="530dp"
                android:orientation="vertical"
                app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
                app:layout_constraintVertical_bias="0.83">

                <EditText
                    android:id="@+id/editTextFirstName"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="First Name"
                    tools:layout_editor_absoluteX="41dp"
                    tools:layout_editor_absoluteY="230dp" />

                <EditText
                    android:id="@+id/editTextLastName"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Last Name"
                    tools:layout_editor_absoluteX="41dp"
                    tools:layout_editor_absoluteY="280dp" />

                <EditText
                    android:id="@+id/editTextCompany"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Company"
                    tools:layout_editor_absoluteX="41dp"
                    tools:layout_editor_absoluteY="330dp" />

                <EditText
                    android:id="@+id/editTextStellung"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Regional Manager"
                    tools:layout_editor_absoluteX="41dp"
                    tools:layout_editor_absoluteY="380dp" />

                <EditText
                    android:id="@+id/editTextMobile"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Mobile"
                    tools:layout_editor_absoluteX="40dp"
                    tools:layout_editor_absoluteY="480dp" />

                <EditText
                    android:id="@+id/editTextStellung2"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Regional Manager"
                    tools:layout_editor_absoluteX="41dp"
                    tools:layout_editor_absoluteY="380dp" />

                <EditText
                    android:id="@+id/editTextMobile2"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Mobile"
                    tools:layout_editor_absoluteX="40dp"
                    tools:layout_editor_absoluteY="480dp" />

                <EditText
                    android:id="@+id/editTextCompany2"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Company"
                    tools:layout_editor_absoluteX="41dp"
                    tools:layout_editor_absoluteY="330dp" />

                <EditText
                    android:id="@+id/editTextFirstName2"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="First Name"
                    tools:layout_editor_absoluteX="41dp"
                    tools:layout_editor_absoluteY="230dp" />

                <EditText
                    android:id="@+id/editTextLastName2"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Last Name"
                    tools:layout_editor_absoluteX="41dp"
                    tools:layout_editor_absoluteY="280dp" />

                <EditText
                    android:id="@+id/editTextPhone"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Phone"
                    tools:layout_editor_absoluteX="40dp"
                    tools:layout_editor_absoluteY="530dp" />

                <EditText
                    android:id="@+id/editTextAge"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Age" />

                <EditText
                    android:id="@+id/editTextPLZ"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Postcode" />

                <EditText
                    android:id="@+id/editTextLand"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Country" />

                <EditText
                    android:id="@+id/editTextOrt"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="City" />

                <EditText
                    android:id="@+id/editTextStrasse"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Street" />

                <EditText
                    android:id="@+id/editTextHausnummer"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="House number" />

                <EditText
                    android:id="@+id/editTextNotiz"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:text="Commentsr" />
            </LinearLayout>
        </ScrollView>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>